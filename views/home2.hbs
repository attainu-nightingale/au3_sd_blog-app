   {{!-- Carrousel --}}
   <div id="demo" class="carousel slide" data-ride="carousel" style="position: relative;top:50px;">

  <!-- Indicators -->
  <ul class="carousel-indicators">
    <li data-target="#demo" data-slide-to="0" class="active"></li>
    <li data-target="#demo" data-slide-to="1"></li>
    <li data-target="#demo" data-slide-to="2"></li>
  </ul>
  
  <!-- The slideshow -->
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://images.unsplash.com/photo-1483721310020-03333e577078?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1100&q=80" width="1100" height="500">
      <div class="carousel-caption">
        <h1>Teamwork Makes The Dream Work</h1>
      </div>
    </div>
    <div class="carousel-item">
      <img src="https://images.unsplash.com/photo-1499781350541-7783f6c6a0c8?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1115&q=80" alt="Chicago" width="1100" height="500">
         <div class="carousel-caption">
        <h1>Art is as Natural as Sunshine and as Vital as Nourishment</h1>
      </div>
    </div>
    <div class="carousel-item">
      <img src="https://images.unsplash.com/photo-1496247749665-49cf5b1022e9?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1053&q=80" alt="New York" width="1100" height="500">
      <div class="carousel-caption">
        <h1>Quality only happens when you care enough to give your best</h1>
      </div>
      </div>
  </div>
  
  <!-- Left and right controls -->
  <a class="carousel-control-prev" href="#demo" data-slide="prev">
    <span class="carousel-control-prev-icon"></span>
  </a>
  <a class="carousel-control-next" href="#demo" data-slide="next">
    <span class="carousel-control-next-icon"></span>
  </a>
</div>




   {{!-- Cards --}}
   {{#if view}}
  <div class="container" style="position: relative;top:90px;">
       <h3><i class="fas fa-fire" style="color: orangered"></i> Most Viewed </h3>
       <hr>
        <div class="row">
           {{#each mostviewd}}
           <div class="col-md-4">
             <a href="/user/posts/{{this._id}}">
            <div class="card" style="margin-left :10px; margin-bottom:50px;border-radius: 15px;transition: 0.3s;">
            <img class="card-img-top" src="{{this.headImg}}" alt="Card image cap" height="200px" style="border-radius: 15px 15px 0 0;">
            <div class="card-body">
            <h5 class="card-title card-title text-truncate" style="max-width: 250px;">
                <font face = "Bedrock" size = "5">{{this.heading}}</font></h5>
            <img src="data:image/png;base64," style="width: 20px;height:20px"><span style="position:relative;top:8px;font-weight:lighter">{{this.date}}</span></img>
            <hr>
            <h6>Author: <span style="font-weight:lighter">{{{this.username}}}</span></h6>
            <h6>Category: <span style="font-weight:lighter">{{{this.category}}}</span></h6>
            {{!-- <h6>Views: <span style="font-weight:lighter">{{{this.views}}}</span></h6> --}}
            <h6><i class="fas fa-eye"></i><span style="padding-left:5px;14px">{{this.views}}</span></h6>
            
         </div>
       </div>
       </a>
       </div>
  
           {{/each}}
           </div>
            <div class="container" style="position: relative;top:35px;">
              <h3><i class="fas fa-rss-square"></i> Recently Uploaded </h3><hr>
        <div class="row">
           {{#each recent}}
           <div class="col-md-4">
             <a href="/user/posts/{{this._id}}">
            <div class="card" style="margin-left :10px; margin-bottom:50px;border-radius: 15px;transition: 0.3s;">
            <img class="card-img-top" src="{{this.headImg}}" alt="Card image cap" height="200px" style="border-radius: 15px 15px 0 0;">
            <div class="card-body">
            <h5 class="card-title card-title text-truncate" style="max-width: 250px;">
                <font face = "Bedrock" size = "5">{{this.heading}}</font></h5>
            <img src="data:image/png;base64," style="width: 20px;height:20px"><span style="position:relative;top:8px;font-weight:lighter">{{this.date}}</span></img>
            <hr>
            <h6>Author: <span style="font-weight:lighter">{{{this.username}}}</span></h6>
            <h6>Category: <span style="font-weight:lighter">{{{this.category}}}</span></h6>
            {{!-- <h6>Views: <span style="font-weight:lighter">{{{this.views}}}</span></h6> --}}
            <h6><i class="fas fa-eye"></i><span style="padding-left:5px;14px">{{this.views}}</span></h6>
            
         </div>
       </div>
       </a>
       </div>
  
           {{/each}}
           </div>
           </div>
           {{/if}}
            <div class="container" style="position: relative;top:70px;">
        <div class="row">
           {{#each data}}
           <div class="col-md-4">
             <a href="/user/posts/{{this._id}}">
            <div class="card" style="margin-left :10px; margin-bottom:50px;border-radius: 15px;transition: 0.3s;">
            <img class="card-img-top" src="{{this.headImg}}" alt="Card image cap" height="200px" style="border-radius: 15px 15px 0 0;">
            <div class="card-body">
            <h5 class="card-title card-title text-truncate" style="max-width: 250px;">
                <font face = "Bedrock" size = "5">{{this.heading}}</font></h5>
            <img src="data:image/png;base64," style="width: 20px;height:20px"><span style="position:relative;top:8px;font-weight:lighter">{{this.date}}</span></img>
            <hr>
            <h6>Author: <span style="font-weight:lighter">{{{this.username}}}</span></h6>
            <h6>Category: <span style="font-weight:lighter">{{{this.category}}}</span></h6>
            {{!-- <h6>Views: <span style="font-weight:lighter">{{{this.views}}}</span></h6> --}}
            <h6><i class="fas fa-eye"></i><span style="padding-left:5px;14px">{{this.views}}</span></h6>
            
         </div>
       </div>
       </a>
     </div>
  
           {{/each}}
           </div>
           </body>
</html>
