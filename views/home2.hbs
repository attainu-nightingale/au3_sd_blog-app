      <button class="btn my-2 my-sm-0" type="submit" style="background-color: #E5D4ED">Search</button>
    </form>
    </div>
        {{!-- <p style="position:relative;left:870px;top:8px;color:whitesmoke"><sup>Signed as {{username}}</sup></p> --}}
        {{else}}
      <li class="nav-item">
        <a class="nav-link" href="/user/signup.html">SignUp</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/user/signup.html">SignIn</a>
      </li>
      <div class="search" style="position:relative;left:970px;">
          <form class="form-inline my-2 my-lg-0" action="/user/posts" method="GET">
      <input class="form-control mr-sm-2" type="text" placeholder="Search Blog By Name, Author Name, Category.. " aria-label="Search" name="search">
      <button class="btn my-2 my-sm-0" type="submit" style="background-color: #E5D4ED">Search</button>
    </form>
    </div>
      {{/if}}
    </ul>
  </div>
</nav>
</nav>
